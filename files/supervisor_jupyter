startretries=10
user=tensorframe
redirect_stderr=false
stdout_logfile=/var/log/jupyter/jupyter.log
stdout_capture_maxbytes=32MB
stderr_logfile=/var/log/jupyter/jupyter.error.log
stderr_capture_maxbytes=32MB
